<h2>Tweets for: <%= @character.name %></h2>


<% @tweets.each do |tweet| %>
  <%= tweet.body %>
  <%= tweet.post_at %>
  <%= link_to "edit", [:edit, @character, tweet], :class => "button" %>
  <%= link_to "destroy", [@character, tweet], :method => :delete,
    :confirm => "Are you sure?", :class => "button"  %></br>
<% end %>
